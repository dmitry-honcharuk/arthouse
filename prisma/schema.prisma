datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  @@map("users")

  id String @id @db.Uuid @default(uuid())

  email String @unique

  googleId String? @unique @map("google_id")

  facebookId String? @unique @map("facebook_id")
}
