{
  "name": "remix-example-basic",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/tailwind.css -o app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o app/styles/tailwind.css",
    "start": "prisma migrate deploy && remix-serve build",
    "setup": "prisma migrate dev"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "1.3.5",
    "@remix-run/react": "1.3.5",
    "@remix-run/serve": "1.3.5",
    "google-auth-library": "^7.14.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "1.3.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/md5": "^2.3.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.10.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  }
}
